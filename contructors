import 'dart:io';
class add{
    add(){
        stdout.write("enter first number=");
        int num1=int.parse(stdin.readLineSync()!);
        stdout.write("enter second number=");
        int num2=int.parse(stdin.readLineSync()!);
        int sum=num1+num2;
        print(sum);
    }
}

void main(){
    add ojb=add();  //ojb here is nickname for a constructor to call
}
